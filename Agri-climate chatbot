        "# 1️⃣ DATA & CONTEXT SETUP\n",
        "# ---------------------------------------------------------\n",
        "# Note: The dataframes are loaded here, but the core context for QA is the 'context' string.\n",
        "rainfall_url = \"https://raw.githubusercontent.com/plotly/datasets/master/2016-weather-data-seattle.csv\"\n",
        "\n",
        "# Attempt to load external data (for completeness, though not strictly used in QA context)\n",
        "try:\n",
        "    rainfall = pd.read_csv(rainfall_url)\n",
        "    print(\"Rainfall dataset loaded:\", rainfall.shape)\n",
        "except Exception as e:\n",
        "    # This is a safe fallback print statement\n",
        "    print(\"Could not load rainfall dataset (safe to ignore if context is hardcoded):\", e)\n",
        "\n",
        "# Hardcoded Crop Data (The source of truth for the QA context)\n",
        "crop_data = pd.DataFrame({\n",
        "    \"Crop\": [\"Rice\", \"Wheat\", \"Maize\", \"Sugarcane\", \"Cotton\"],\n",
        "    \"Ideal_Temperature\": [\"25-35°C\", \"10-25°C\", \"18-27°C\", \"20-35°C\", \"25-35°C\"],\n",
        "    \"Rainfall_mm\": [\"1000-2000\", \"500-1000\", \"500-800\", \"1200-1500\", \"400-700\"],\n",
        "    \"Soil_Type\": [\"Clayey\", \"Loamy\", \"Alluvial\", \"Loamy\", \"Black\"],\n",
        "    \"State\": [\"West Bengal\", \"Punjab\", \"Bihar\", \"Uttar Pradesh\", \"Gujarat\"] # <--- NEW STATE DATA\n",
        "})\n",
        "\n",
        "# Construct the knowledge base (context) for the QA model\n",
        "context_parts = [\"Rainfall observations are taken from Seattle weather data 2016.\"]\n",
        "for _, row in crop_data.iterrows():\n",
        "    # Context updated to include state information\n",
        "    context_parts.append(\n",
        "        f\"{row.Crop} grows well at {row.Ideal_Temperature} with {row.Rainfall_mm} mm rainfall, prefers {row.Soil_Type} soil, and is primarily grown in {row.State}.\"\n",
        "    )\n",
        "context = \" \".join(context_parts)\n",
        "print(\"QA Context created:\", context)\n",
        "\n",
        "# ---------------------------------------------------------\n",
        "# 2️⃣ MODEL INITIALIZATION\n",
        "# ---------------------------------------------------------\n",
        "# Load the Question Answering pipeline using the specified model\n",
        "print(\"Loading DistilBERT QA model...\")\n",
        "try:\n",
        "    qa_model = pipeline(\"question-answering\", model=\"distilbert-base-cased-distilled-squad\")\n",
        "    print(\"Model loaded successfully.\")\n",
        "except Exception as e:\n",
        "    print(f\"Error loading model: {e}\")\n",
        "    # Define a dummy model to prevent the app from crashing if model loading fails\n",
        "    def dummy_qa(question, context): return {\"answer\": \"Model failed to load. Check dependencies and network.\"}\n",
        "    qa_model = lambda **kwargs: dummy_qa(**kwargs)\n",
        "\n",
        "# ---------------------------------------------------------\n",
        "# 3️⃣ CHATBOT LOGIC\n",
        "# ---------------------------------------------------------\n",
        "def agri_chatbot(question):\n",
        "    \"\"\"Answers a question based on the predefined context using the QA model.\"\"\"\n",
        "    if not question:\n",
        "        return \"Please ask a question about crops, temperature, rainfall, soil, or state.\"\n",
        "\n",
        "    try:\n",
        "        # Pass the user's question and the comprehensive context to the QA model\n",
        "        result = qa_model(question=question, context=context)\n",
        "\n",
        "        # The model returns the most confident span of text from the context as the answer\n",
        "        answer = result.get(\"answer\", \"I could not find a relevant answer in the available crop data.\")\n",
        "\n",
        "        # Simple formatting improvements\n",
        "        if answer and len(answer) > 1 and answer[0].islower():\n",
        "            answer = answer[0].upper() + answer[1:]\n",
        "\n",
        "        return answer\n",
        "    except Exception as e:\n",
        "        return f\"An error occurred during inference: {e}\"\n",
        "\n",
        "# New wrapper function to process the query and clear the input field\n",
        "def process_query_and_clear(query):\n",
        "    \"\"\"Calls the main chatbot logic and returns the answer and an empty string to clear the input.\"\"\"\n",
        "    answer = agri_chatbot(query)\n",
        "    # The first return value updates response_output, the second clears query_input\n",
        "    return answer, \"\"\n",
        "\n",
        "# Define the clearing functions\n",
        "def clear_all():\n",
        "    \"\"\"Clears both input and output fields.\"\"\"\n",
        "    return \"\", \"\"\n",
        "\n",
        "def handle_close():\n",
        "    \"\"\"Displays a goodbye message and clears the input field.\"\"\"\n",
        "    return \"Goodbye! 👋\", \"\" # Output message, clear input field\n",
        "\n",
        "# ---------------------------------------------------------\n",
        "# 4️⃣ GRADIO INTERFACE\n",
        "# ---------------------------------------------------------\n",
        "with gr.Blocks(title=\"Agri-Climate QA Chatbot\") as demo:\n",
        "    gr.Markdown(\"# 🌾 Context-Based Agri-Climate Chatbot\")\n",
        "    gr.Markdown(\n",
        "        \"\"\"\n",
        "        This chatbot uses a **DistilBERT** Question-Answering model. It can answer questions\n",
        "        based only on the following **hardcoded crop data**, which now includes state information:\n",
        "\n",
        "        | Crop | State | Temp | Rainfall | Soil |\n",
        "        | :--- | :--- | :--- | :--- | :--- |\n",
        "        | Rice | West Bengal | 25-35°C | 1000-2000 mm | Clayey |\n",
        "        | Wheat | Punjab | 10-25°C | 500-1000 mm | Loamy |\n",
        "        | Maize | Bihar | 18-27°C | 500-800 mm | Alluvial |\n",
        "        | Sugarcane | Uttar Pradesh | 20-35°C | 1200-1500 mm | Loamy |\n",
        "        | Cotton | Gujarat | 25-35°C | 400-700 mm | Black |\n",
        "\n",
        "        **Try questions like:** * *Which state primarily grows Rice?*\n",
        "        * *Which crop is grown in Bihar?*\n",
        "        * *Which state requires the highest rainfall for its main crop?*\n",
        "        \"\"\"\n",
        "    )\n",
        "\n",
        "    # Chat interface setup\n",
        "    with gr.Row():\n",
        "        query_input = gr.Textbox(label=\"Your Question\", lines=2, placeholder=\"e.g., Which crop needs high rainfall?\")\n",
        "\n",
        "    with gr.Row():\n",
        "        submit_button = gr.Button(\"Get Answer\", variant=\"primary\")\n",
        "        clear_button = gr.Button(\"Clear\")\n",
        "        close_button = gr.Button(\"Close\")\n",
        "\n",
        "    # --- Visual break added here ---\n",
        "    gr.Markdown(\"---\")\n",
        "\n",
        "    response_output = gr.Textbox(label=\"Chatbot Answer\", lines=4, interactive=False)\n",
        "\n",
        "    # Define interactions - outputs now include response_output AND query_input (for clearing)\n",
        "    submit_button.click(\n",
        "        process_query_and_clear,\n",
        "        inputs=query_input,\n",
        "        outputs=[response_output, query_input]\n",
        "    )\n",
        "    query_input.submit(\n",
        "        process_query_and_clear,\n",
        "        inputs=query_input,\n",
        "        outputs=[response_output, query_input]\n",
        "    )\n",
        "\n",
        "    # Clear button binding\n",
        "    clear_button.click(clear_all, outputs=[response_output, query_input])\n",
        "\n",
        "    # Close button binding - now calls handle_close to set the goodbye message\n",
        "    close_button.click(handle_close, outputs=[response_output, query_input])\n",
        "\n",
        "demo.launch(debug=True)\n"
      ]
    }
  ]
}